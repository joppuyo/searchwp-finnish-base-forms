{
    "name": "joppuyo/searchwp-finnish-base-forms",
    "type": "wordpress-plugin",
    "description": "SearchWP plugin to add Finnish base forms in search index",
    "license": "GPL-3.0-or-later",
    "minimum-stability": "dev",
    "require": {
        "symfony/process": "v4.4.44",
        "yahnis-elsts/plugin-update-checker": "^5.1",
        "danielstjules/stringy": "^3.1",
        "siiptuo/voikko": "dev-master"
    },
    "config": {
        "platform": {
            "php": "7.4.33"
        }
    },
    "autoload": {
        "psr-4": {
            "NPX\\FinnishBaseForms\\": "src/"
        }
    },
    "repositories":[
        {
            "type": "vcs",
            "url": "https://github.com/siiptuo/voikko-php"
        }
    ]
}
